<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans
    	http://www.springframework.org/schema/beans/spring-beans.xsd
    	http://www.springframework.org/schema/context
    	http://www.springframework.org/schema/context/spring-context.xsd
">


    <bean
            id = "ClassicalMusicBean"
            class="com.maximzheleznyak.springcourse.ClassicalMusic">

    </bean>

    <bean
            id = "RockMusicBean"
            class="com.maximzheleznyak.springcourse.RockMusic">
    </bean>


    <bean
            id = "MusicPlayer"
            class="com.maximzheleznyak.springcourse.MusicPlayer">
<!--            <constructor-arg ref="RockMusicBean"/>-->
        <constructor-arg ref ="ClassicalMusicBean"/>



    </bean>






</beans>